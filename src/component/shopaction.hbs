<div class='row my-5'>
  {{#with service}}
    <div class='col-12'>
      <div class='card text-center text-bg-success'>
        <div class='card-body'>
          <h2>{{title}}</h2>
          <p>{{description}}</p>
          {{#each buttons}}
            <a
              href='{{link}}'
              class='btn btn-primary'
              target='_blank'
              rel='{{rel}}'
            >{{text}}</a>
          {{/each}}
        </div>
      </div>
    </div>
  {{/with}}
</div>